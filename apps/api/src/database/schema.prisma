generator client {
    provider = "prisma-client-js"
    output = "./generate/client"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

enum UserRole {
  OWNER
  ADMIN
  MEMBER
}


model User {
    id         String   @id @unique @default(uuid())
    email      String   @unique
    password   String
    fullName   String   @map("full_name")
    image      String
    telegramId String   @unique @map("telegrma_id")
    username   String   @unique
    language   String
    cratedAt   DateTime @default(now()) @map("created_at")
    updatedAt  DateTime @default(now()) @updatedAt

    @@map("users")
}
